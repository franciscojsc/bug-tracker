{
    "version": 2,
    "env": {
        "PLANILHA_GOOGLE":"@planilha-google-bug-tracker"
    },
    "builds": [
        {
            "src": "*.js",
            "use": "@now/node-server"
        }
    ],
    "routes":[
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ]
}